#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>


/ {
    chosen {
		zephyr,console = &cdc_acm_uart;
	};

    keymap {
        compatible = "zmk,keymap";
        default_layer  {
            bindings = <
                &kp A &kp B &kp C
                &kp E &kp F &kp G 
                &kp H &kp I &kp J
            >;
        };
    };
};


// Allow USB logging
&usbd {
	status = "okay";
	cdc_acm_uart: cdc_acm_uart {
		compatible = "zephyr,cdc-acm-uart";
		label = "CDC_ACM_0";
	};
};